<?xml version="1.0" encoding="utf-8"?>
<animated-selector android:constantSize="true"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false" android:state_pressed="true">
        <bitmap android:gravity="center" android:src="@drawable/scrubber_control_off_pressed_mtrl_alpha" />
    </item>
    <item android:state_enabled="false">
        <bitmap android:gravity="center" android:src="@drawable/scrubber_control_off_mtrl_alpha" />
    </item>
    <item android:state_pressed="true" android:id="@android:id/pressed">
        <bitmap android:gravity="center" android:src="@drawable/scrubber_control_to_pressed_mtrl_005" android:tint="?android:colorControlActivated" />
    </item>
    <item android:id="@android:id/unpressed">
        <bitmap android:gravity="center" android:src="@drawable/scrubber_control_to_pressed_mtrl_000" android:tint="?android:colorControlActivated" />
    </item>
    <transition android:toId="@android:id/pressed" android:fromId="@android:id/unpressed">
        <animation-list>
            <item android:duration="15">
                <bitmap android:gravity="center" android:src="@drawable/scrubber_control_to_pressed_mtrl_000" android:tint="?android:colorControlActivated" />
            </item>
            <item android:duration="15">
                <bitmap android:gravity="center" android:src="@drawable/scrubber_control_to_pressed_mtrl_001" android:tint="?android:colorControlActivated" />
            </item>
            <item android:duration="15">
                <bitmap android:gravity="center" android:src="@drawable/scrubber_control_to_pressed_mtrl_002" android:tint="?android:colorControlActivated" />
            </item>
            <item android:duration="15">
                <bitmap android:gravity="center" android:src="@drawable/scrubber_control_to_pressed_mtrl_003" android:tint="?android:colorControlActivated" />
            </item>
            <item android:duration="15">
                <bitmap android:gravity="center" android:src="@drawable/scrubber_control_to_pressed_mtrl_004" android:tint="?android:colorControlActivated" />
            </item>
            <item android:duration="15">
                <bitmap android:gravity="center" android:src="@drawable/scrubber_control_to_pressed_mtrl_005" android:tint="?android:colorControlActivated" />
            </item>
        </animation-list>
    </transition>
    <transition android:toId="@android:id/unpressed" android:fromId="@android:id/pressed">
        <animation-list>
            <item android:duration="15">
                <bitmap android:gravity="center" android:src="@drawable/scrubber_control_from_pressed_mtrl_000" android:tint="?android:colorControlActivated" />
            </item>
            <item android:duration="15">
                <bitmap android:gravity="center" android:src="@drawable/scrubber_control_from_pressed_mtrl_001" android:tint="?android:colorControlActivated" />
            </item>
            <item android:duration="15">
                <bitmap android:gravity="center" android:src="@drawable/scrubber_control_from_pressed_mtrl_002" android:tint="?android:colorControlActivated" />
            </item>
            <item android:duration="15">
                <bitmap android:gravity="center" android:src="@drawable/scrubber_control_from_pressed_mtrl_003" android:tint="?android:colorControlActivated" />
            </item>
            <item android:duration="15">
                <bitmap android:gravity="center" android:src="@drawable/scrubber_control_from_pressed_mtrl_004" android:tint="?android:colorControlActivated" />
            </item>
            <item android:duration="15">
                <bitmap android:gravity="center" android:src="@drawable/scrubber_control_from_pressed_mtrl_005" android:tint="?android:colorControlActivated" />
            </item>
        </animation-list>
    </transition>
</animated-selector>