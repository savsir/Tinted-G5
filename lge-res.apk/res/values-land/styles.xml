<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Theme_LGE_Default_BottomSheet">
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowAnimationStyle">@style/Animation_LGE_Default_BottomSheet</item>
        <item name="android:background">#d9ffffff</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:windowMinWidthMajor">79.99999%</item>
        <item name="android:windowMinWidthMinor">79.99999%</item>
        <item name="android:listPreferredItemPaddingLeft">?android:dialogPreferredPadding</item>
        <item name="android:listPreferredItemPaddingRight">?android:dialogPreferredPadding</item>
        <item name="android:listPreferredItemPaddingStart">?android:dialogPreferredPadding</item>
        <item name="android:listPreferredItemPaddingEnd">?android:dialogPreferredPadding</item>
        <item name="android:windowTranslucentNavigation">false</item>
    </style>
    <style name="Animation_LGE_Default_BottomSheet">
        <item name="android:windowEnterAnimation">@anim/lgdialog_enter</item>
        <item name="android:windowExitAnimation">@anim/lgdialog_exit</item>
    </style>
</resources>
